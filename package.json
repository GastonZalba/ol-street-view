{
  "name": "ol-street-view",
  "version": "1.1.5",
  "description": "Street View implementation for Open Layers",
  "browser": "dist/ol-street-view.min.js",
  "main": "dist/ol-street-view.js",
  "module": "lib/ol-street-view.js",
  "types": "lib/ol-street-view.d.ts",
  "style": "lib/ol-street-view.css",
  "files": [
    "index.js",
    "lib/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "watch": "rollup -w --config rollup.config.dist.js --dev",
    "doc": "documentation readme src/ol-street-view.ts --shallow --quiet=true --access=public --access=undefined --section=API --parse-extension ts && node scripts/external-links.js README.md && prettier --write README.md",
    "format": "eslint --fix src/*.ts && prettier --write src",
    "lint": "eslint src/*.ts && prettier --check src",
    "build": "npm run lint && rollup --config rollup.config.dist.js && rollup --config rollup.config.js",
    "postbuild": "del-cli lib/components dist/components"
  },
  "keywords": [
    "ol",
    "openlayers",
    "street view",
    "google street view"
  ],
  "author": "GastÃ³n Zalba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GastonZalba/ol-street-view.git"
  },
  "bugs": {
    "url": "https://github.com/GastonZalba/ol-street-view/issues"
  },
  "homepage": "https://github.com/GastonZalba/ol-street-view#readme",
  "dependencies": {
    "google-maps": "^4.3.3",
    "interactjs": "^1.10.11"
  },
  "peerDependencies": {
    "ol": ">=5.0.0",
    "interactjs": ">=1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "clean-css": "^4.2.3",
    "del-cli": "^3.0.1",
    "documentation": "^13.2.5",
    "eslint-config-openlayers": "^14.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ol": "^6.5.0",
    "prettier": "^2.2.1",
    "rollup": "^2.33.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  }
}
